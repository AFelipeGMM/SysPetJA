<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
    <managed-bean>
        <managed-bean-name>enderecoMB</managed-bean-name>
        <managed-bean-class>bean.EnderecoMB</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <navigation-rule>
        <from-view-id>/manterEndereco.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manterEndereco</from-outcome>
            <to-view-id>/manterEndereco.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

	<managed-bean>
        <managed-bean-name>funcionarioMB</managed-bean-name>
        <managed-bean-class>bean.FuncionarioMB</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <navigation-rule>
        <from-view-id>/manterFuncionario.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manterFuncionario</from-outcome>
            <to-view-id>/manterFuncionario.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
</faces-config>
